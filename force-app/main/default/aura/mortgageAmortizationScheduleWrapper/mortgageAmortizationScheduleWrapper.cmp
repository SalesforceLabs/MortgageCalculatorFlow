<aura:component implements="lightning:availableForFlowScreens" access="global">
    <aura:attribute name="mortgageRate" type="Decimal" access="global"/>
    <aura:attribute name="mortgageYears" type="Integer" access="global"/>
    <aura:attribute name="mortgagePrincipal" type="Decimal" access="global"/>

    <c:mortgageAmortizationSchedule principal="{!v.mortgagePrincipal}" rate="{!v.mortgageRate}" years="{!v.mortgageYears}"/>
</aura:component>	
